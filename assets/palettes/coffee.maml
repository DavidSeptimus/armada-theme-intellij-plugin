  "ColorPhilosophy": {
    "description": "Coffee Light theme - A warm latte-inspired palette with light editor on dark brown UI"
    "principles": [
      "Dark brown UI (Coffee_10-40) frames the workspace like a rich espresso cup"
      "Light cream editor (Coffee_160-150) provides a clean, paper-like writing surface"
      "Deep green (#3d8a45) for strings - rich coffee bean color with good contrast"
      "Golden yellow (#9d8232) for numbers - warm honey tones in your coffee"
      "Teal blue (#2b6b7a) for keywords - calm and professional"
      "Deep violet (#6b5fc8) for types/functions - elegant and distinct"
      "Magenta (#b5478a) for parameters/attributes - rich berry notes"
      "Coral red (#b8453a) for control flow/enums - attention-grabbing warmth"
      "Muted cyan (#2d5a7f) for function calls - cool contrast"
      "Forest green (#3d6738) for operators/properties - grounded and natural"
      "Burnt orange (#c66735) for decorators/annotations - spice notes"
      "Medium brown (#5c5548) for comments - subtle like coffee sediment"
    ]
    "accessibility": {
      "contrast": "All highlight colors provide strong contrast against light backgrounds (Coffee_160-150)"
      "color_blindness": "Darker saturated colors chosen for better distinction on light backgrounds"
      "bright_light": "Light editor reduces glare while dark UI frame provides visual anchoring"
    }
  }

  "UsageNotes": {
    "editor_background": "Coffee_160 to Coffee_150 - light cream/paper color"
    "ui_chrome": "Coffee_10 to Coffee_40 - dark brown for toolbars, sidebars, status bar"
    "ui_panels": "Coffee_50 to Coffee_70 - medium brown for secondary panels"
    "selection": "Coffee_140 with transparency"
    "line_highlight": "Coffee_150 with low opacity"
    "search_highlight": "#9d8232 with 25% opacity (golden highlight)"
    "error": "#b8453a"
    "warning": "#c66735"
    "info": "#2d5a7f"
    "success": "#3d8a45"
    "visual_hierarchy": "Dark frame + light content = classic book or coffee shop menu aesthetic"
  }
