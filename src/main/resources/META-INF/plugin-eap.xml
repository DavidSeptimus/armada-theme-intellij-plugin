<!-- EAP-specific plugin configuration -->
<!-- This file is only included in EAP builds and provides experimental features -->
<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
      <themeProvider
              id="com.github.davidseptimus.armada.armada-dark-purple"
              path="themes/armada-dark-purple/armada-dark-purple.theme.json"
              targetUi="new"
      />

      <themeProvider
              id="com.github.davidseptimus.armada.armada-dark-purle-islands"
              path="themes/armada-dark-purple/armada-dark-purple-islands.theme.json"
              targetUi="islands"
      />

      <themeProvider
              id="com.github.davidseptimus.armada.armada-dark-purple-classic-ui"
              path="themes/armada-dark-purple/armada-dark-purple-classic-ui.theme.json"
              targetUi="classic"
      />

      <themeProvider
              id="com.github.davidseptimus.armada.armada-dark-azure"
              path="themes/armada-dark-azure/armada-dark-azure.theme.json"
              targetUi="new"
      />

      <themeProvider
              id="com.github.davidseptimus.armada.armada-dark-azure-islands"
              path="themes/armada-dark-azure/armada-dark-azure-islands.theme.json"
              targetUi="islands"
      />

      <themeProvider
              id="com.github.davidseptimus.armada.armada-dark-azure-classic-ui"
              path="themes/armada-dark-azure/armada-dark-azure-classic-ui.theme.json"
              targetUi="classic"
      />

      <themeProvider
              id="com.github.davidseptimus.armada.armada-coffee-light"
              path="themes/armada-coffee-light/armada-coffee-light.theme.json"
              targetUi="new"
      />

      <themeProvider
              id="com.github.davidseptimus.armada.armada-coffee-light-islands"
              path="themes/armada-coffee-light/armada-coffee-light-islands.theme.json"
              targetUi="islands"
      />

      <themeProvider
              id="com.github.davidseptimus.armada.armada-coffee-light-classic-ui"
              path="themes/armada-coffee-light/armada-coffee-light-classic-ui.theme.json"
              targetUi="classic"
      />
  </extensions>
</idea-plugin>

